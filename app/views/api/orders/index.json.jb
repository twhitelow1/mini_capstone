json = []
index = 0
while index < @orders.length
  order = @orders[index]
  order_hash = {
    id: order.id,
    user_id: order.user_id,
    product_id: order.product_id,
    quantity: order.quantity,
    subtotal: order.subtotal,
    tax: order.tax,
    total: order.total,
  }
  index += 1
  json << order_hash
end

return json
